{
  "name": "puppet-etherpad",
  "version": "2.0.0",
  "author": "Vox Pupuli",
  "summary": "Install and configure Etherpad",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-etherpad",
  "project_page": "https://github.com/voxpupuli/puppet-etherpad",
  "issues_url": "https://github.com/voxpupuli/puppet-etherpad/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    },
    {
      "name": "puppetlabs-vcsrepo",
      "version_requirement": ">= 1.3.1 < 2.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.1 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
